module HDU(IDEX_memRead, ID_BranchOrJump, IFID_write);
	input IDEX_memRead, ID_BranchOrJump;
	output IFID_write;

	reg IFID_writeReg;
	
	initial
		IFID_writeReg = 1;

	assign IFID_write = IFID_writeReg;
	
	always@(*) begin
		if(IDEX_memRead | ID_BranchOrJump)
			IFID_writeReg = 0;
		else
			IFID_writeReg = 1;
	end
	

endmodule
